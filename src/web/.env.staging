VITE_NODE_ENV=staging

# API Configuration
VITE_API_BASE_URL=https://api-staging.dollarfunding.com/api/v1
VITE_API_TIMEOUT=30000
VITE_UPLOAD_TIMEOUT=60000
VITE_MAX_RETRIES=3
VITE_RETRY_DELAY=1000

# Authentication Configuration
VITE_AUTH_DOMAIN=auth-staging.dollarfunding.com
VITE_AUTH_CLIENT_ID=staging_client_id
VITE_AUTH_AUDIENCE=https://api-staging.dollarfunding.com
VITE_ENABLE_MFA=true
VITE_SESSION_TIMEOUT=1800

# Service URLs
VITE_STORAGE_URL=https://storage-staging.dollarfunding.com
VITE_OCR_SERVICE_URL=https://ocr-staging.dollarfunding.com

# Application Settings
VITE_LOG_LEVEL=debug
VITE_ENABLE_MOCK_API=false

# Security Headers
VITE_STRICT_TRANSPORT_SECURITY=max-age=31536000; includeSubDomains
VITE_CONTENT_SECURITY_POLICY=default-src 'self'
VITE_XSS_PROTECTION=1; mode=block
VITE_CONTENT_TYPE_OPTIONS=nosniff
VITE_FRAME_OPTIONS=DENY
VITE_REFERRER_POLICY=strict-origin-when-cross-origin

# Rate Limiting
VITE_RATE_LIMIT_MAX_REQUESTS=1000
VITE_RATE_LIMIT_WINDOW_MS=3600000

# Error Tracking
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_PERFORMANCE_TRACKING=true

# Document Processing
VITE_MAX_UPLOAD_SIZE=25000000
VITE_ALLOWED_FILE_TYPES=application/pdf
VITE_MAX_DOCUMENTS_PER_REQUEST=10

# Feature Flags
VITE_ENABLE_DOCUMENT_PREVIEW=true
VITE_ENABLE_BATCH_PROCESSING=true
VITE_ENABLE_REAL_TIME_UPDATES=true